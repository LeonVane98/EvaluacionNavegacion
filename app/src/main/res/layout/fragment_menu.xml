<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MenuFragment">

    <!-- TODO: Update blank fragment layout -->
    <Button
        android:id="@+id/btnMMM"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Media Moda Mediana"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <Button
        android:id="@+id/btnFactorial"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Factorial"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/btnMMM"/>
    <Button
        android:id="@+id/btnVerificarMultiplo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Verificar Multiplo"
        app:layout_constraintTop_toBottomOf="@+id/btnFactorial"
        android:layout_marginTop="25dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>